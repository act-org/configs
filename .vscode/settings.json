{
  "editor.codeActionsOnSave": [
    "source.organizeImports",
    "source.fixAll"
  ],
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.tabSize": 2,
  "[mdx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "myplugin",
    "cursi",
    "commandsstop",
    "someplugin",
    "someuser",
    "oclif",
    "pluginsinspect",
    "pluginsinstall",
    "pluginslink",
    "pluginsuninstall",
    "tocstop",
    "usagestop",
    "postpack",
    "posttest",
    "weeden",
  ]
}
